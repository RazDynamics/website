                        <div class="title">Track 4</div>
                        <div class="subtitle">Room 4</div>
                    </a>
                </li>
            </ul>
            <!-- Parent Tab panes -->
            <div class="tab-content">
                <div class="tab-pane fade in active" id="TrackOne">
                    <div class="tab-content">
                        <div class="tab-pane fade in active">
                            <div class="panel-group timeline-schedule" id="panelTimelineOne">

                                @{var index = 0;}
                                @model.tracks.track1.forEach(function(m)
                                {
                                    @{index++}
                                    <div class="panel panel-default">
                                        <div class="speaker-wrapper">
                                        @if (m.author.avatarUrl == undefined)
                                        {
                                            <img src="/images/blank_avatar.jpg" class="img-responsive img-circle" alt="@m.author.firstname @m.author.lastname" />
                                        }else{
                                            <img src="@m.author.avatarUrl" class="img-responsive img-circle" alt="@m.author.firstname @m.author.lastname" />
                                        }
                                        </div>
                                        <div class="panel-heading">
                                            <div class="panel-title">
                                                <div class="time">
                                                    <i class="fa fa-clock-o"></i> @{ var data = new Date(m.time) } @data.getHours():@(("0" + data.getMinutes()).substr(-2))
                                                </div>
                                                <a data-toggle="collapse" data-parent="#panelTimelineOne" href="#item@index" class="collapsed">
                                                @m.title
                                                    <div class="pull-right fa fa-angle-up"></div>
                                                    <div class="pull-right fa fa-angle-down"></div>
                                                </a>
                                          </div>
                                      </div>
                                      <div id="item@index" class="panel-collapse collapse">
                                          <div class="panel-body">
                                              <article>
                                                  <p>@m.description</p>

                                                    @if(m.author.firstname != "" && m.author.firstname != undefined)
                                                    {
                                                        <p class="author">

                                                            @{var url = (m.author.blog != "" && m.author.blog != undefined) ? m.author.blog : "http://twitter.com/" + m.author.twitter;}
                                                            <a href="@url" target="_blank">
                                                                <strong>
                                                                    <i class="fa fa-user"></i> 
                                                                    @m.author.firstname @m.author.lastname
                                                                </strong>
                                                            </a>
                                                        </p>
                                                    }

                                                  
                                                  
                                                    @if(m.tags.length > 0){
                                                        <p class="tags"><strong><i class="fa fa-tags"></i> @m.tags</strong></p>
                                                    }
                                                  
                                              </article>
                                          </div>
                                      </div>
                                  </div>
                                })

                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
