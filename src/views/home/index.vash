<!-- Stats -->
<section id="stat" class="section light">
    <div class="overlay"></div>
    <div class="container">
        <div class="row">
            <div class="col-md-3 col-sm-3 col-xs-6">
                <div class="counter animated hiding" data-animation="fadeInDown" data-delay="0">
                    <div class="stat"><span class="value" data-from="0" data-to="20">20</span>
                    </div>
                    <div class="stat-info">hours</div>
                    <p class="small">Get ready to experience an intense day packed with lots of hours of good content, knowledge exchange and fun.</p>
                </div>
            </div>
            <div class="col-md-3 col-sm-3 col-xs-6">
                <div class="counter animated hiding" data-animation="fadeInDown" data-delay="500">
                    <div class="stat"><span class="value" data-from="0" data-to="20">20</span>
                    </div>
                    <div class="stat-info">speakers</div>
                    <p class="small">We are expecting international speakers: the CFP is open now, expect to know more soon.</p>
                </div>
            </div>
            <div class="col-md-3 col-sm-3 col-xs-6 tablet-clear-left">
                <div class="counter animated hiding" data-animation="fadeInDown" data-delay="1000">
                    <div class="stat"><span class="value" data-from="0" data-to=10>10</span>+</div>
                    <div class="stat-info">technologies</div>
                    <p class="small">ASP.NET vNext, AngularJS, Node.js, Ruby, Git, Javascript, and much more: all modern web development technologies will be represented.</p>
                </div>
            </div>
            <div class="col-md-3 col-sm-3 col-xs-6">
                <div class="counter animated hiding" data-animation="fadeInDown" data-delay="1500">
                    <div class="stat"><span class="value" data-from="0" data-to="4">4</span>
                    </div>
                    <div class="stat-info">halls</div>
                    <p class="small">To provide the best experience, we'll have a big plenary hall and other rooms for the sessions through the day.</p>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- End Stats -->

<!-- Schedule Section -->
<section id="schedule" class="section dark">
    <div class="container">
        <div class="icon-wrap"><span class="icon icon-active icon-text-hierarchy-07"></span></div>
        <h3>Full schedule</h3>
        <h6>All talks will be in english</h6>
        <br />
        <section class="nav-tabs-default">
            <!-- Parent Nav Tabs -->
            <ul class="nav nav-tabs">
                <li class="active">
                    <a href="#TrackOne" data-toggle="tab">
                        <div class="title">Track 1</div>
                        <div class="subtitle">Room 1</div>
                    </a>
                </li>
                <li>
                    <a href="#TrackTwo" data-toggle="tab">
                        <div class="title">Track 2</div>
                        <div class="subtitle">Room 2</div>
                    </a>
                </li>
