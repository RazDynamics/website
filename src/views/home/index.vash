<!-- Speakers Section -->
<section id="speakers" class="section light">
    <div class="overlay"></div>
    <div class="container">
        <div class="icon-wrap"><span class="icon icon-active icon-multimedia-12"></span></div>
        <h3>Our speakers</h3>
        <div class="sub-title">Look at our top speakers</div>
        <br />
        <br />

        <section class="speakers-carousel">
            <div id="speakers-carousel">

                @model.speakers.forEach(function(m)
                {
                    <div class="speaker-item-container">
                        <div class="speaker-item animated hiding" data-animation="fadeInUp" data-delay="0">
                            <div class="img-wrapper">
                                <img src="@m.avatarUrl" class="img-responsive img-circle" alt="@m.firstname @m.lastname" width="142" height="142" />
                            </div>
                            <div class="name">@m.firstname @m.lastname</div>
                            <div class="sub">@m.role</div>
                            <p class="small">@m.bio</p>
                            <div class="social-link">
                                @if(m.twitter && m.twitter != ""){
                                    <a href="http://www.twitter.com/@m.twitter"><span class="fa fa-twitter"></span></a>
                                }
                                @if(m.site && m.site != ""){
                                    <a href="@m.site"><span class="fa fa-link"></span></a>
                                }
                                
                            </div>
                        </div>
                    </div>
                })

            </div>
            <a class="carousel-nav carousel-prev"><i class="fa fa-angle-left fa-3x"></i></a>
            <a class="carousel-nav carousel-next"><i class="fa fa-angle-right fa-3x"></i></a>
        </section>
        <!--<div class="btn-wrapper">
          <a href="#" class="btn btn-sm btn-default">See all speakers</a>
        </div>-->
    </div>
</section>
<!-- End Speakers Section -->
<!-- Price Section -->
<section id="price" class="section dark">
      <div class="container">
        <div class="icon-wrap"><span class="icon icon-active icon-shopping-02"></span></div>
        <h3>Pricetable</h3>
        <div class="sub-title">The 4 options to attend the conference</div>
		<br>
		<br>
        <div class="pricing-cols pricing-4-cols">

          <div class="pricing-col animated fadeInLeft visible" data-animation="fadeInLeft" data-delay="0">
              <div class="pricing-col-header">
